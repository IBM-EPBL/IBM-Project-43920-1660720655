import random 
x=random.randint (1,100) 
y=random.randint (1,50) 
print("random temperature and humidity reading")
print(x)
print(y)


if( (x>50) & (y<30)) :
  print("temperature is high:",x,"%") 
  print ("humidity is high:",y,"%")
  print ("Alarm Detected") 
elif( (x>50) & (y>30)):
  print("temperature is high:",x,"%") 
  print ("humidity is low:",y,"%")
  print ("Alarm Detected") 
